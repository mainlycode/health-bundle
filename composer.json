{
  "name": "mainlycode/health-bundle",
  "description": "Symfony bundle for adding a /health endpoint to your application",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "othillo",
      "email": "othillo@othillo.nl"
    }
  ],
  "require": {
    "php": ">=7.3",
    "symfony/http-foundation": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.2",
    "symfony/browser-kit": "^5.2",
    "symfony/yaml": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "MainlyCode\\HealthBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MainlyCode\\HealthBundle\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
